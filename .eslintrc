{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "standard-with-typescript",
      "plugin:react/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": "./tsconfig.json" 
    },
    "plugins": [
      "react",
      "@typescript-eslint" 
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"

    }
  }
  